{
    "name": "react-static-boilerplate",
    "private": true,
    "scripts": {
        "start": "tsc-watch --p tsconfig.static.json --onFirstSuccess \"react-static start -c tmp/static.config.js\"",
        "stage": "npm run compile:config && react-static build -c tmp/static.config.js --staging",
        "build": "npm run compile:config  && react-static -c tmp/static.config.js build --production",
        "bundle": "npm run compile:config && react-static -c tmp/static.config.js bundle",
        "export": "npm run compile:config && react-static -c tmp/static.config.js export",
        "compile:config": "tsc --p tsconfig.static.json",
        "serve": "serve dist -p 3000 -s",
        "svgr": "svgr --config-file svgrrc.config.js --ext tsx -d src/app/elements/svg-elements public/assets/svg-elements",
        "contentful:pull": "contentful:pull-types",
        "contentful:push": "contentful:push-types",
        "storybook": "start-storybook -p 9001 -s ./public -c .storybook",
        "contentful-typescript-codegen": "contentful-typescript-codegen --output src/contentful/@types/contentful.d.ts"
    },
    "dependencies": {
        "@contentful/rich-text-react-renderer": "^13.4.0",
        "@reach/router": "^1.2.1",
        "@svgr/cli": "^4.3.2",
        "@types/js-cookie": "^2.2.4",
        "@zxing/library": "^0.15.2",
        "body-scroll-lock": "^2.6.4",
        "broadcast-channel": "^3.0.3",
        "contentful": "^7.9.1",
        "csg-toolbox": "./scripts/contentful/",
        "dotenv": "^8.1.0",
        "file-replace-loader": "^1.0.1",
        "flatted": "^2.0.1",
        "js-cookie": "^2.2.1",
        "moment": "^2.24.0",
        "normalize.css": "^8.0.1",
        "reach-router-hash-history": "^0.0.3",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-gtm-module": "^2.0.8",
        "react-input-mask": "^2.0.4",
        "react-markdown": "^4.3.1",
        "react-on-visible": "^1.6.0",
        "react-static": "^7.2.3",
        "react-static-plugin-reach-router": "^7.2.0",
        "react-static-plugin-sass": "^7.2.2",
        "react-static-plugin-scss-modules": "^1.0.4",
        "react-static-plugin-sitemap": "^7.2.0",
        "react-static-plugin-source-filesystem": "^7.2.0",
        "react-static-plugin-typescript": "^7.2.0",
        "react-transition-group": "^4.3.0"
    },
    "devDependencies": {
        "@storybook/addon-info": "^5.3.13",
        "@storybook/react": "^5.3.13",
        "@types/node": "^12.7.2",
        "@types/reach__router": "^1.2",
        "@types/react": "^16.9.1",
        "@types/react-dom": "^16.8.5",
        "@types/react-hot-loader": "^4.1.0",
        "@types/react-redux": "^7.1.2",
        "@types/redux": "^3.6.0",
        "@types/storybook__react": "^4.0.2",
        "@types/webpack-env": "^1.14.0",
        "awesome-typescript-loader": "^5.2.1",
        "aws-sdk": "2.524.0",
        "babel-loader": "^8.0.6",
        "contentful-management": "^5.11.3",
        "contentful-typescript-codegen": "^2.0.1",
        "mime-types": "2.1.24",
        "node-sass": "^4.12.0",
        "prettier": "^1.18.2",
        "react-docgen-typescript-loader": "^3.2.1",
        "react-hot-loader": "^4.12.11",
        "serve": "^11.1.0",
        "tsc-watch": "^4.0.0",
        "tslint": "5.11.0",
        "tslint-config-prettier": "1.15.0",
        "typescript": "^3.5.3"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "all"
    }
}
